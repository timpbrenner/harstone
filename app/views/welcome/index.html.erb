<script language="javaScript">
  var added_parts = [];

  function add_parts() {
    var name = $('#part_name').html();
    var count = $('#part_count').val();
    var size = $('.size_selector.active').html();

    added_parts.push({ 'name':name, 'count':count, 'size':size });
    update_used_parts();

    $('.size_selector').removeClass('active');
    $('#part_count').val(1)
    $('#partModal').modal('hide');
  }

  function add_ft_parts() {
    var name = $('#ft_part_name').html();
    var count = $('#ft_part_count').val() + '\'';

    added_parts.push({ 'name':name, 'count':count });
    update_used_parts();

    $('#ft_part_count').val(1)
    $('#ftPartModal').modal('hide');
  }

  function update_used_parts() {
    var inner = '';

    $.each(added_parts, function(index, value) {
      inner += "<div class='used_part'>"
      if(value.size) {
        inner += value.size + ' '; 
      }

      inner += value.name.replace(/_/g, ' ') + ' -' + value.count + "</div>";
    });

    $('#parts_used').html(inner);
  }

  function updatePartsBox() {
    var part_strings = [];

    $('.used_part').each(function() {
      part_strings.push($(this).html());
    });

    $('#field-dcadf34b4948181').html(part_strings.join());
    $('#myModal').modal('hide');
  }
</script>

