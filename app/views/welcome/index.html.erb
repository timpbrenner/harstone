function loadPopupHelper() {
  var table_string = "<table><tr><th></th>";
  for (column = 0; column < sizes.length; column++)
  {
    if(column % 2 == 0)
      table_string += "<th class='top'>";
    else
      table_string += "<th class='top alt'>";
    
    table_string += sizes[column].replace("ft", "'").replace('in', '"').replace("_", " ");
    table_string += "</th>";
  }
  table_string += "</tr>";
  for (row = 0; row < types.length; row++)
  {
    if(row % 2 == 0)
      table_string += "<tr><th>";
    else
      table_string += "<tr class='alt'><th>";
    
    table_string += types[row].replace("_", " ");
    table_string += "</th>";
    for (column = 0; column < sizes.length; column++)
    {
      var input_tag = prices[sizes[column] + "_" + types[row]] + " x " + "<input class='qty' type='text' size=3 "
           + "id='" + sizes[column] + "_" + types[row] + "' onblur='submit_popup();' />";

      if(column % 2 != 0 && row % 2 != 0)
        table_string += "<td class='dark'>";
      else if(column % 2 == 0)
        table_string += "<td>";
      else
        table_string += "<td class='alt'>";

      table_string += input_tag;
      table_string += "</td>";
    }
    table_string += "</tr>";
  }
  table_string += "</table>";
  document.getElementById('popup_table').innerHTML = table_string;
}

function load_feet() {
  var new_table = 10;


  var table_string = "<table><tr><th></th><th>price</th><th>amount</th></tr>";
  var index = 0;
  for (var key in feet_types) {
    if (index % new_table == 0 && index > 0)
      table_string += "</table><table><tr><th></th><th>price</th><th>amount</th></tr>";

    if(index % 2 == 0)
      table_string += "<tr>";
    else
      table_string += "<tr class='alt'>";

    table_string += "<th>" + key.replace('_', ' ').replace('_', ' ') + "</th>"
    table_string += "<td>" + feet_types[key] + "</td>"
    table_string += "<td><input id=" + key + " class='qty' size=5 onblur='submit_popup();' /></td>"
    table_string += "</tr>";
    index++;
  }
  table_string += "</table>"

  document.getElementById('feet_table').innerHTML = table_string;
}

function submit_popup() {
  var text = "";

  for (row = 0; row < sizes.length; row++)
  {
    for (column = 0; column < types.length; column++)
    {
      var input_tag = sizes[row] + "_" + types[column];
      var val = document.getElementById(input_tag);
      if(val.value != '')
      {
        prices[input_tag];
        
        text += (val.value + " ");
        text += (sizes[row] + " ");
        text += (types[column] + " -");
        var amount = parseInt(val.value) * parseFloat(prices[sizes[row] + "_" + types[column]]);
        amount = Math.round(amount*Math.pow(10,2))/Math.pow(10,2);
        text += (amount + ", ");
      }
    }
  }

  for (var key in feet_types) {
    var feet_val = document.getElementById(key);
    if(feet_val.value != '') {
      text += (feet_val.value + "' ");
      text += (key + " -");
      var amount = parseInt(feet_val.value) * parseFloat(feet_types[key]);
      amount = Math.round(amount*Math.pow(10,2))/Math.pow(10,2);
      text += (amount + ", ");
    }
  }

  while(text.indexOf("in") >= 0 || text.indexOf("ft") >= 0 || text.indexOf("_") >= 0) 
  {
    text = text.replace("ft", "'").replace('in', '"').replace('_', ' ');
  }

  document.getElementById('field-dcadf34b4948181').value = text.substring(0, text.length - 2);
}

function show_element(element_name) {
  document.getElementById(element_name).style.visibility = 'visible';
}

function hide_element(element_name) {
  document.getElementById(element_name).style.visibility = 'collapsed';
}

function toggle_show(element_name) {
  display = document.getElementById(element_name).style.display;
  if (display == 'none') {
    document.getElementById(element_name).style.display = 'block';
  } else {
    document.getElementById(element_name).style.display = 'none';
  }
}

