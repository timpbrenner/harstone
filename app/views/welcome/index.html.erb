      '#_1_THHN', '1_OT', '2_OT', '3_OT', '4_OT', 'MCM_250', 'MCM_300', 'MCM_350', 'MCM_500', 'MCM_600', 'MCM_750', 'MCM_750',
      '14/2', '14/3', '12/2', '12/3', '12/4', '10/2', '10/3', '10/4'];

  var orig = '';
  for (i = 0; i < parts.length; i++) {
    orig += '<div class="part_button" onclick="select_part(\'' + parts[i] + '\')">' + parts[i].replace(/_/g, ' ') + "</div>";
  }
  $('#part_container').html(orig);

  var ft_orig = '';
  for (i = 0; i < ft_parts.length; i++) {
    ft_orig += '<div class="ft_part_button" onclick="select_ft_part(\'' + ft_parts[i] + '\')">' + ft_parts[i].replace(/_/g, ' ') + "</div>";
  }
  $('#ft_part_container').html(ft_orig);


  function select_part(part) {
    $('#part_name').html(part);
    $('#partModal').modal();
  }

  function select_ft_part(part) {
    $('#ft_part_name').html(part);
    $('#ftPartModal').modal();
  }

$('.size_selector').click(function(event) {
    $('.size_selector').removeClass('active');
    $(event.target).addClass('active');

    $('#add_part_submit').removeClass('disabled');
  } );
</script>
